{
  "name": "mdx-butler",
  "version": "0.5.7",
  "description": "",
  "private": false,
  "repository": "https://github.com/NicoZweifel/mdx-butler",
  "type": "module",
  "license": "MIT",
  "author": "Nico Zweifel",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "types": "./build/index.d.ts"
    },
    "./client": {
      "import": "./build/client.js",
      "types": "./build/client.d.ts"
    }
  },
  "scripts": {
    "publish": "pnpm publish -r",
    "version": "pnpm version -r",
    "prepublish": "npm-run-all build:clean build",
    "compile": "tsc",
    "compile:watch": "tsc",
    "watch": "npm-run-all compile:watch",
    "build:clean": "rimraf ./build",
    "build": "npm-run-all compile",
    "format": "prettier **/*.{cjs,js,ts,tsx,css,json,md,mdx} --write",
    "lint": "eslint **/*.{ts,tsx,mdx} --fix",
    "test": "vitest"
  },
  "keywords": [],
  "peerDependencies": {
    "mdx-bundler": ">= 10.0.0"
  },
  "dependencies": {
    "estree-util-build-jsx": "3.0.0",
    "glob": "^10.5.0",
    "mdast-util-mdx": "^3.0.0",
    "mdast-util-mdx-expression": "^2.0.1",
    "mdast-util-to-markdown": "^2.1.2",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@types/node": "^20.19.25",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@types/unist": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "esbuild": "^0.19.12",
    "eslint": "^8.57.1",
    "eslint-plugin-mdx": "^2.3.4",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vitest": "^0.3.26",
    "mdx-bundler": "^10.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.10",
    "typescript": "^5.9.3",
    "vitest": "^1.6.1"
  }
}
